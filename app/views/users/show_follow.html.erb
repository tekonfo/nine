          <% @users.each do |user| %>
          <div class="user_avatars">
<!--             <span><img src="<%#= user.avatar.url(:thumb) %>"></span><br /> -->
            <%= link_to user.name ,user_path(user) %>
          </div>
          <% end %>
      <% end %>
    </section>
  </aside>
</div>
